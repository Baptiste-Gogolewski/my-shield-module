/ {
    behaviors {
        semi_dot: mod_morph_semidot {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp COMMA &kp KP_DOT>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };

        colon_slash: mod_morph_colonslash {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp DOT &kp KP_DIVIDE>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };

        percent_unicode: percent_unicode_behavior {
            compatible = "zmk,behavior-unicode";
            #binding-cells = <0>;
            unicode = <0x25>;
        };
    };
};
